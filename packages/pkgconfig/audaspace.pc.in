prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: audaspace
Description: Audaspace is a feature rich high level audio library
Requires: 
Version: @AUDASPACE_VERSION@
Libs: -L${libdir} -laudaspace @CAUDASPACE_LIBRARY@ @PYAUDASPACE_LIBRARY@
Cflags: -I${prefix}/include/audaspace
