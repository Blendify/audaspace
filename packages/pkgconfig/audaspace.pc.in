prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: audaspace
Description: Audaspace is a feature rich high level audio library
Requires: 
Version: @AUDASPACE_VERSION@
Libs: -L${libdir} -laudaspace @AUDASPACE_C_LIBRARY@ @AUDASPACE_PY_LIBRARY@
Cflags: -I${includedir}/audaspace
